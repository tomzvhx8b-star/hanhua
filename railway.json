{
  "$schema": "https://railway.app/schema.json",
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "python -m pagermaid",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  },
  "env": {
    "PGM_LANGUAGE": "zh-cn",
    "WEB_ENABLE": "True",
    "WEB_HOST": "0.0.0.0",
    "WEB_PORT": "3333"
  }
}
